{
  "name": "ssh2-fs-promise",
  "version": "0.1.0",
  "description": "Promise wrappers for ssh2-fs functions.",
  "main": "bin/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "watch 'npm run -s build' src -du",
    "lint": "echo '=> linting' && eslint src && echo '=> finished linting'",
    "pretranspile": "npm run -s lint",
    "transpile": "echo '=> transpiling' && mkdir -p bin && babel src --out-dir bin && echo '=> finished transpiling'",
    "build": "echo '\n\n\n=> building' && npm run -s transpile && echo '=> finished build'",
    "postbuild": "npm link"
  },
  "author": {
    "name": "themaskedavenger",
    "email": "themaskedavenger@protonmail.com"
  },
