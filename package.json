  "keywords": [
    "child",
    "process",
    "fs",
    "ssh",
    "ssh2"
  ],
  "homepage": "https://github.com/themaskedavenger/ssh2-fs-promise",
  "repository": {
    "type": "git",
    "url": "https://github.com/themaskedavenger/ssh2-fs-promise.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "ssh2-connect": "^2.0.4",
    "ssh2-fs": "^0.3.6"
